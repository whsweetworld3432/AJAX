<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>QueryProvincesServlet</servlet-name>
        <servlet-class>com.HelloJQuery.controller.QueryProvinceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryProvincesServlet</servlet-name>
        <url-pattern>/queryProvinces</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QueryCityServlet</servlet-name>
        <servlet-class>com.HelloJQuery.controller.QueryCityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryCityServlet</servlet-name>
        <url-pattern>/queryCitys</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OneServlet</servlet-name>
        <servlet-class>com.HelloJQuery.controller.OneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OneServlet</servlet-name>
        <url-pattern>/oness</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OneServletTestJson</servlet-name>
        <servlet-class>com.HelloJQuery.controller.OneServletTestJson</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OneServletTestJson</servlet-name>
        <url-pattern>/onesJson</url-pattern>
    </servlet-mapping>
</web-app>